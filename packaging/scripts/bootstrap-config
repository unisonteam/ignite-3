LOGS_PATH=$IGNITE_HOME/log
LIBS_PATH=$IGNITE_HOME/lib
BIN_PATH=$IGNITE_HOME/bin
CONFIG_PATH=$IGNITE_HOME/etc

LOG_OUT_FILE=$LOGS_PATH/${NODE_NAME}.log

# Export these variables so that IgniteCliRunner can use them as default values
export IGNITE_NODE_NAME=defaultNode
export IGNITE_WORK_DIR=$IGNITE_HOME/work
export IGNITE_CONFIG_PATH=$CONFIG_PATH/ignite-config.conf
